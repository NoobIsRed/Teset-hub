local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Welcome To Tester Script!", HidePremium = false, IntroEnabled = false,IntroText = "Tester Hub",  SaveConfig = true, ConfigFolder = "OrionTest"})

_G.Buy = true

function  buyfood()
	while _G.Buy == true do
		local ohString1 = "Floppa Food"
local ohString2 = "the_interwebs"
game:GetService("ReplicatedStorage").Events.Unlock:FireServer(ohString1, ohString2)
	end
end

-- Tabs
local Main = Window:MakeTab({
   Name = "Main",
   Icon = "rbxassetid://4483345998",
   PremiumOnly = false
})

local Misc = Window:MakeTab({
   Name = "Misc",
   Icon = "rbxassetid://4483345998",
   PremiumOnly = false
})

-- Toggles
Main:AddToggle({
   Name = "Auto Click",
   Default = false,
   Callback = function(Value)
      print(Value)
   end    
})

Main:AddToggle({
    Name = "Buy Food",
   Default = false,
   Callback = function(Value)
      _G.Buy = true
	  buyfood()
   end
})

Misc:AddToggle({
   Name = "COMING SOON"
   Default = false,
   Callback = function(Value)
      print(Value)
   end
})
